<config>
<fvm>
  <name>
    kh
  </name>
  <platform>cuda</platform>
  <grid>
    <lowerCorner>0 0 0</lowerCorner>
    <upperCorner>1 1 0</upperCorner>
    <dimension>1024 1024 1</dimension>
  </grid>
  <boundary>periodic</boundary>
  <flux>hll3</flux>
  <endTime>4</endTime>
  <equation>euler2</equation>
  <equationParameters>
    <gamma>1.4</gamma>
  </equationParameters>
  <reconstruction>weno2</reconstruction>
  <cfl>auto</cfl>
  <integrator>auto</integrator>

  <initialData>
    <python>euler_brownian.py</python>
    <parameters>
      <parameter>
        <name>X</name>
        <length>2097152</length>
        <values><value>0</value></values>
      </parameter>
    </parameters>
  </initialData>
  <diffusion>
    <name>none</name>
    <reconstruction>none</reconstruction>
  </diffusion>
</fvm>
<uq>
  <samples>1024</samples>
  <generator>auto</generator>
  <parameters>
    <parameter>
      <name>X</name>
      <length>2097152</length>
      <type>normal</type>
    </parameter>
  </parameters>
  <stats>
  <stat>
    <name>
      meanvar
    </name>
    <numberOfSaves>1</numberOfSaves>
    <writer>
      <type>netcdf</type>
      <basename>brownianeuler</basename>
    </writer>
    </stat>

<stat>
  <name>
    structure_basic
  </name>
  <numberOfSaves>1</numberOfSaves>
  <direction>0</direction>
  <numberOfH>32</numberOfH>
  <p>1</p>
  <writer>
    <type>netcdf</type>
    <basename>brownianeuler_x</basename>
  </writer>
  </stat>

<stat>
  <name>
    structure_basic
  </name>
  <numberOfSaves>1</numberOfSaves>
  <direction>1</direction>
  <numberOfH>32</numberOfH>
  <p>1</p>
  <writer>
    <type>netcdf</type>
    <basename>brownianeuler_y</basename>
  </writer>
  </stat>

<stat>
  <name>
    structure_cube
  </name>
  <numberOfSaves>16</numberOfSaves>
  <numberOfH>32</numberOfH>
  <p>1</p>
  <writer>
    <type>netcdf</type>
    <basename>brownianeuler_cube</basename>
  </writer>
  </stat>

<stat>
  <name>
    structure_cube
  </name>
  <numberOfSaves>16</numberOfSaves>
  <numberOfH>32</numberOfH>
  <p>2</p>
  <writer>
    <type>netcdf</type>
    <basename>brownianeuler_cube</basename>
  </writer>
  </stat>

<stat>
  <name>
    structure_cube
  </name>
  <numberOfSaves>16</numberOfSaves>
  <numberOfH>32</numberOfH>
  <p>3</p>
  <writer>
    <type>netcdf</type>
    <basename>brownianeuler_cube</basename>
  </writer>
  </stat>

  </stats>
</uq>
</config>
